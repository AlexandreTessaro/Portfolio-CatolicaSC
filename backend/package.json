{
  "name": "startup-collab-backend",
  "version": "1.0.0",
  "description": "Backend para aplicação de colaboração de startups",
  "main": "index.js",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint . --ext .js",
    "format": "prettier --write .",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run --testPathPattern=integration",
    "db:migrate": "node scripts/migrate.js",
    "db:seed": "node scripts/seed.js",
    "db:seed-synthetic": "node scripts/generate-synthetic-data.js",
    "db:seed-realistic": "node scripts/generate-realistic-data.js",
    "db:seed-simple": "node scripts/generate-simple-data.js",
    "db:seed-working": "node scripts/generate-working-data.js",
    "db:insert-data": "node insert-data.js",
    "db:seed-scalable": "node scripts/generate-scalable-data.js",
    "db:test-accuracy": "node test-recommendation-accuracy.js",
    "db:clear": "node scripts/data-manager.js clear",
    "db:stats": "node scripts/data-manager.js stats",
    "db:users": "node scripts/data-manager.js users",
    "db:projects": "node scripts/data-manager.js projects",
    "db:matches": "node scripts/data-manager.js matches",
    "db:export": "node scripts/data-manager.js export",
    "db:examples": "node scripts/examples.js",
    "db:test-synthetic": "node test-synthetic-data.js",
    "test:audit": "node scripts/test-audit.js",
    "test:load": "artillery run artillery-config.yml",
    "test:load:report": "artillery run --output load-report.json artillery-config.yml && artillery report load-report.json",
    "sonar": "sonar-scanner",
    "sonar:local": "sonar-scanner -Dsonar.host.url=http://localhost:9000"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "artillery": "^2.0.26",
    "cross-env": "^10.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.2.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "firebase-admin": "^12.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "serverless-http": "^3.2.0",
    "socket.io": "^4.7.2",
    "winston": "^3.11.0"
  }
}
